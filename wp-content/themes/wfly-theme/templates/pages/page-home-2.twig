{% extends 'layout.twig' %}
{% block content %}
    {% if post.title and (title_option != "on") %}
    <div id="page-title" class="page-title">
      <h1>{{ post.title }}</h1>
    </div>
  {% endif %}
 {{ shortcode('[custom_shortcode]') }}
   <div class="post-content">
    {{  post.post_content|shortcodes }}
  </div>
{% endblock %}
